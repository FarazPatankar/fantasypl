{
  "name": "fpl-api",
  "version": "4.0.3",
  "description": "A wrapper for Fantasy Premier League API",
  "author": "Jeppe Smith <mail@jeppesmith.dk>",
  "license": "MIT",
  "private": false,
  "repository": "https://github.com/jeppe-smith/fpl-api",
  "scripts": {
    "test": "jest",
    "make:schema": "npx ts-json-schema-generator --path src/types.ts --out schema.json",
    "build": "npm run build:node & npm run build:web",
    "build:node": "tsc --project tsconfig.build.json",
    "build:web": "webpack"
  },
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": "./dist/node/index.js",
    "./web": "./dist/web/index.js"
  },
  "typesVersions": {
    "*": {
      "web": [
        "dist/node/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/superagent": "^4.1.15",
    "jest": "^28.1.3",
    "jsonschema": "^1.4.1",
    "ts-jest": "^28.0.7",
    "ts-json-schema-generator": "^1.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "superagent": "^8.0.0"
  }
}
